  "version": "0.2.0",
  "configurations": [
    {
      "type": "node-terminal",
      "name": "Run Server",
      "request": "launch",
      "command": "npm run server",
      "cwd": "${workspaceFolder}"
    },
    {
      "type": "node-terminal",
      "name": "Run Host",
      "request": "launch",
      "command": "npm run host",
      "cwd": "${workspaceFolder}"
